{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/projets",
      "dest": "/projets.html"
    },
    {
      "src": "/equipe",
      "dest": "/equipe.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 